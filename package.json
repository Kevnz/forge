{
  "name": "forge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest --config=jest.config.js --detectOpenHandles --forceExit",
    "lint": "eslint ./src",
    "watch": "npm-run-all --parallel dev:**",
    "build": "webpack --config ./src/webpack/prod.config.js --mode production",
    "dev:ui": "webpack-dev-server --config ./src/webpack/dev.config.js --mode development",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:e2e": "isom test:e2e",
    "test:e2e:commit": "isom test:e2e:commit",
    "test:e2e:dev": "isom test:e2e:dev"
  },
